    <!-- http://purl.org/aiaontology#Auditor -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Auditor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/aiaontology#compliesWithRequirements"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Control(n)"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/aiaontology#isVerifierOf"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Claim(n)"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Verifier"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Auditor</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A set of predefined behaviours for a certified agent who conducts an audit.</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.org/aiaontology#Auditor"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#equivalentClass"/>
        <owl:annotatedTarget>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/aiaontology#compliesWithRequirements"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Control(n)"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/aiaontology#isVerifierOf"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Claim(n)"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">The requirement for an Agent to fulfill the Role of Auditor is that they verify some Claim AND complies with the (legal) Controls for being an Auditor</rdfs:comment>
    </owl:Axiom>
    


