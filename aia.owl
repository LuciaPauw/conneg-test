    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasControl -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasControl">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Control(n)"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasDuration -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasDuration">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Duration"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasEnvironment -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasEnvironment">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:comment xml:lang="en">The Environment for which the Parameter&apos;s value was measured/calculated</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasFormula -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasFormula">
