    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasSubject">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control(n)"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasSymbol -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasSymbol"/>
    


    <!-- http://purl.org/aiaontology#hasType -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasType">
