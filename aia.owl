        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Role"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operator</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A set of predefined behaviours for an agent who operates a specific thing (e.g., an instrument) in the context of a specific activity.</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.org/aiaontology#Operator"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#equivalentClass"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/aiaontology#operates"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Instrument"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">The requirement for an Agent to fulfill the Role of Operator is that they operate some Instrument.</rdfs:comment>
    </owl:Axiom>
    


