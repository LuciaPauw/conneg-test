        <rdfs:range rdf:resource="http://purl.org/aiaontology#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasCondition">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control(n)"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#State"/>
